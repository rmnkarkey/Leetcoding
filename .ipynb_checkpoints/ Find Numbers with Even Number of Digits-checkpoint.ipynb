    "class Solution(object):\n",
    "    def findNumbers(self, nums):\n",
    "        new_arr = []\n",
    "        for i in nums:\n",
    "            if len(str(i)) % 2 ==0:\n",
    "                new_arr.append(i)\n",
    "        return len(new_arr)\n",
    "\n",
    "    \n",
    "s=Solution()\n",
    "s.findNumbers([555,901,482,1771])\n",
    "        "
   ]
  }

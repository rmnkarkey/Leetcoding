    "def lists(a,val):\n",
    "    num = 0\n",
    "    last_elm = a[len(a)-1]\n",
    "    s = a[len(a)-1]\n",
    "    gap = val - s\n",
    "    if val % s == 0:\n",
    "        return s\n",
    "    if gap < s:\n",
    "        num = gap\n",
    "        return num\n",
    "    elif gap > s:\n",
    "        return lists(a,gap)\n",
